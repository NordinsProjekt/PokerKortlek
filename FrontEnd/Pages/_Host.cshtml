@page "/"
@namespace FrontEnd.Pages
@addTagHelper *, Microsoft.AspNetCore.Mvc.TagHelpers
@{
    Layout = "_Layout";
}
<component type="typeof(App)" render-mode="Server" />
<script>
    function autoScroll()
    {
        var objDiv = document.getElementById("scrollbox");
        objDiv.scrollTop = objDiv.scrollHeight;
    }
</script>
